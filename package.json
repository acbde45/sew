    "marked": "^4.0.14",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "vite": "^2.9.5",
    "vooks": "^0.2.12"
  },
  "devDependencies": {
    "naive-ui": "^2.28.0",
